{
  "version": 2,
  "builds": [
    { "src": "app.py", "use": "@vercel/python" },
    { "src": "index.html", "use": "@vercel/static" },
    {
      "src": "/src/(.*\\.(js|css|png|jpg|jpeg|gif|svg))$"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/api/(.*)", "dest": "/app.py" }
  ]
}
